<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>School Class Form</title>
  </head>
  <body>
    <%= form_for SchoolClass.new do |f| %>
      <%= f.label :title %>
      <%= f.text_field :title %>
      <%= f.label :room_number %>
      <%= f.text_field :room_number %>
      <%= f.submit %>
    <% end %>
  </body>
</html>
